<!--顶部-->
<header class="header">
    <a href="javascript:void (0)" class="back"></a>
    <h2>订单详情</h2>
</header>
 

 
<!--内容-->
<section class="OrderDetails-main">
	<p class="OrderDetails-state"></p>
	<!--收货人信息-->
	<div class="OrderDetails-User">
		<div class="OrderDetails-User-backimage"><img src="images/orderDetails/bg_order_address.png"/></div>
		<div class="OrderDetails-User-counter">
			<div class="OrderDetails-User-name"><span>收货人:</span><span class="OrderDetailsUserName"></span><span class="OrderDetails-User-name-img"><img src="images/orderDetails/pic_name.png"/></span></div>
			<div class="OrderDetails-User-phone"><span>联系方式:</span><span class = "OrderDetailsUserPhone"></span></div>
			<div class="OrderDetails-User-address"><span>收货地址:</span><span  class="OrderDetailsUserAddress"></span></div>
		</div>
	</div>
	<!--物流单号-->
	<div class="OrderDetails-Logistics">
		<p>物流单号:</p>
		<p class="OrderDetails-Logistics-number"></p>
 	</div>
	<!--商品信息-->
	<div class="OrderDetails-commodity">
		<div class="OrderDetails-commodity-title">
			<p class="OrderDetails-commodity-left">
				<span>店铺:</span>
				<span class="OrderDetailsCommodityName"></span>
			</p>
		 
		</div>
		<ul class="OrderDetails-commodity-Content">
		
 		</ul>
	</div>
 	<!--订单-->
	<div class="OrderDetails-Order">
		<div class="Order">
			<p class="Order-Name">订单号：&nbsp;&nbsp;<p class="Order-Order-number"></p></p>
			 <p class="Order-content"></p> 
		</div>
		 
	</div>
	<!--合计-->
	<div class="Total">
		<p class="Total-Name">合计：&nbsp;&nbsp;<span class="Total-Name-number"></span></p>
	</div>
</section>

